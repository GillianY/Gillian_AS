<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <alpha
        android:duration="800"
        android:fromAlpha="1"
        android:toAlpha="0"></alpha>
    <scale
        android:duration="800"
        android:fromXScale="1"
        android:fromYScale="1"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXScale="0"
        android:toYScale="0"></scale>
    <rotate android:fromDegrees="720"
        android:duration="800"
        android:toDegrees="0"
        android:pivotX="50%"
        android:pivotY="50%">
    </rotate>
</set>